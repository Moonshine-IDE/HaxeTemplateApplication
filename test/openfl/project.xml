<?xml version="1.0" encoding="utf-8"?>
<project>
	<meta title="Main" package="Main" version="1.0.0" company="CompanyName" />
	<app main="Main" path="bin" file="Main" />

	<window allow-high-dpi="true" />
	<window fps="30"/>
	<window fps="0" if="html5"/>
	<window width="1128" height="768"/>
	
	<haxelib name="openfl" />
	<haxelib name="actuate" />
	<haxelib name="feathersui" />
	<haxelib name="amfio"/>
	<haxelib name="champaign"/>
	<haxelib name="haxetemplateapplication"/>

	<!-- classpath -->
	<source path="src" />

	<haxeflag name="--macro" value="include('feathers')"/>

	<haxedef name="HXCPP_STACK_LINE" />
	<haxedef name="HXCPP_STACK_TRACE"/>
	<haxedef name="HXCPP_CHECK_POINTER"/>
</project>